<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="BedrockBoot.Controls.ContentDialogContent.DelGameVersionContent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BedrockBoot.Controls.ContentDialogContent"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel Orientation="Horizontal">
        <ProgressRing Margin="0,8,12,0" VerticalAlignment="Top"></ProgressRing>
        <StackPanel Width="300" VerticalAlignment="Top">
            <TextBlock Margin="0,5"
               HorizontalAlignment="Left"
               Name="Del_ProgressTextBlock">0 %</TextBlock>
            <ProgressBar HorizontalAlignment="Stretch"
                 Name="Del_ProgressBar"
                 Margin="0,5"></ProgressBar>
        </StackPanel>
    </StackPanel>
</Page>
